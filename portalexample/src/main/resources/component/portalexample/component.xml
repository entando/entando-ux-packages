<?xml version="1.0" encoding="UTF-8"?>
<component>
	<code>portalexample_card</code>
	<description>Card PortalExample</description>
	<installation>
		<tableMapping>
			<datasource name="servDataSource">
				<class>org.entando.entando.portalexample.aps.system.init.servdb.Card</class>
			</datasource>
		</tableMapping>
		<environment code="production">
			<defaultSqlResources>
				<datasource name="portDataSource">
classpath:sql/misc/portalexample_card/port_data_production.sql
				</datasource>
				<datasource name="servDataSource">
classpath:sql/misc/portalexample_card/serv_data_production.sql
				</datasource>
			</defaultSqlResources>
		</environment>
		<environment code="test">
			<defaultSqlResources>
				<datasource name="portDataSource">
classpath:sql/misc/portalexample_card/port_data_test.sql
				</datasource>
				<datasource name="servDataSource">
classpath:sql/misc/portalexample_card/serv_data_test.sql
				</datasource>
			</defaultSqlResources>
		</environment>
	</installation>
</component>
